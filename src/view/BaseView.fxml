                                             <Insets top="20.0" />
                                         </VBox.margin>
                                     </Label>
                                     <Slider fx:id="simulationSpeed" layoutX="0.0" layoutY="280.0" max="1000" min="20" minorTickCount="10" prefHeight="25.0" prefWidth="300.0" showTickMarks="true" snapToTicks="true" value="20" />
                                 </children>
                                  <VBox.margin>

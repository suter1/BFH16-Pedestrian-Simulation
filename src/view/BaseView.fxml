                               <children>
                                   <ImageView>
                                       <image>
                                           <Image url="/resources/img/pedestrianSim_logo.jpg"/>
                                       </image>
                                   </ImageView>
                                   <VBox alignment="CENTER_LEFT">
                                       <children>
                                           <Label styleClass="logo" text="Pedestrian" />
                                           <Label styleClass="logo" text="Simulation" />
                                       </children>
                                       <HBox.margin>
                                           <Insets left="10.0" />
                                       </HBox.margin>
                                   </VBox>
                               </children>
                           </HBox>
                       </AnchorPane>
                   </children>
                </AnchorPane>

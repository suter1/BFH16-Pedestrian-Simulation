                                               <Insets left="5.0" />
                                           </HBox.margin>
                                       </TextField>
                                   </children>
                                   <VBox.margin>
                                       <Insets bottom="20.0" />
                                   </VBox.margin>
                               </HBox>
                               <CheckBox fx:id="isWeighted" maxHeight="25.0" minHeight="25.0" mnemonicParsing="false" text="use weighted person creation">

                               <Label styleClass="label-header" text="Statistics">
                                   <VBox.margin>
                                       <Insets bottom="10.0" />
                                   </VBox.margin>
                               </Label>
                               <GridPane>
                                   <columnConstraints>
                                       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />

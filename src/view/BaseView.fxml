                                       <Insets bottom="10.0" />
                                   </VBox.margin>
                               </CheckBox>
                               <HBox>
                                   <children>
                                       <Label alignment="TOP_LEFT" maxHeight="25.0" maxWidth="150.0" prefHeight="25.0" prefWidth="150.0" text="under 30 years" textAlignment="CENTER" />
